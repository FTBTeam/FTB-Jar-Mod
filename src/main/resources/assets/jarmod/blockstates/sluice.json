{
	"multipart": [
		{"when": {"facing": "north"}, "apply": {"model": "jarmod:block/sluice/frame"}},
		{"when": {"facing": "south"}, "apply": {"model": "jarmod:block/sluice/frame", "y": 180}},
		{"when": {"facing": "west"}, "apply": {"model": "jarmod:block/sluice/frame", "y": 270}},
		{"when": {"facing": "east"}, "apply": {"model": "jarmod:block/sluice/frame", "y": 90}},
		{"when": {"mesh": "cloth"}, "apply": {"model": "jarmod:block/sluice/cloth_mesh"}},
		{"when": {"mesh": "iron"}, "apply": {"model": "jarmod:block/sluice/iron_mesh"}},
		{"when": {"mesh": "gold"}, "apply": {"model": "jarmod:block/sluice/gold_mesh"}},
		{"when": {"mesh": "diamond"}, "apply": {"model": "jarmod:block/sluice/diamond_mesh"}},
		{"when": {"water": true}, "apply": {"model": "jarmod:block/sluice/water"}}
	]
}